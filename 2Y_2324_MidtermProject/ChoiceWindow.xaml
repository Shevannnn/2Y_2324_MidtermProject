<Window x:Class="_2Y_2324_MidtermProject.ChoiceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_2Y_2324_MidtermProject"
        mc:Ignorable="d"
        Title="ChoiceWindow" Height="100" Width="200">
    <Grid>
        <TextBlock Text="What would you like to add?" Margin="10"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="10">
            <Button Content="Pet" Width="50" Margin="5" Click="Button_Click"/>
            <Button Content="Supply" Width="50" Margin="5" Click="Button_Click_1"/>
        </StackPanel>
    </Grid>
</Window>
